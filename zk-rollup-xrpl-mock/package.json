{
  "name": "zk-rollup-xrpl",
  "version": "1.0.0",
  "description": "Simple ZK Rollup implementation for XRPL EVM",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js --network xrpl-evm-devnet",
    "deploy:testnet": "hardhat run scripts/deploy.js --network xrpl-evm-testnet",
